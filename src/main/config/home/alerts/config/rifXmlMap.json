        },
        "//rif:registryObjects/rif:registryObject/rif:collection/rif:description[@type='rights']": {
            ".":["dc:accessRights.skos:prefLabel", "dc_accessRights"]
        },

        "//rif:registryObjects/rif:registryObject/rif:collection/rif:subject[@type='local']": "dc:subject.vivo:keyword.0.rdf:PlainLiteral",
        "//rif:registryObjects/rif:registryObject/rif:collection/rif:subject[@type='anzsrc-for']":
            {
                ".": "dc:subject.anzsrc:for.0.rdf:resource",
                "@title": "dc:subject.anzsrc:for.0.skos:prefLabel"
            },
        "//rif:registryObjects/rif:registryObject/rif:collection/rif:subject[@type='anzsrc-seo']":
            {
                ".": "dc:subject.anzsrc:seo.0.rdf:resource",
                "@title": "dc:subject.anzsrc:seo.0.skos:prefLabel"
            }

},
